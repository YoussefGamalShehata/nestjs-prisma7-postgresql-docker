generator client {
  // provider   = "prisma-client"
  provider = "prisma-client-js"
  js_extension = true
  output     = "./generated"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  createdAt DateTime @default(now())
}
